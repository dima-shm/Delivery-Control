@model IEnumerable<DelControlWeb.Models.User>
@{
    ViewBag.Title = "Управление пользователями";
}

<p>
    @Html.ActionLink("Добавить пользователя", "Create")
</p>
<table class="table table-striped table-hover">
    <tr>
        <th>ID компании</th>
        <th>Имя пользователя</th>
        <th>Номер телефона</th>
        <th>Адрес</th>
        <th>Email</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.CompanyId</td>
            <td>@item.UserName</td>
            <td>@item.Phone</td>
            <td>@item.Address</td>
            <td>@item.Email</td>
            <td>
                <a href="/UserAccounts/Edit/@item.Id" class="glyphicon glyphicon-pencil" aria-hidden="true"/>
                <a href="/UserAccounts/Details/@item.Id" class="glyphicon glyphicon-eye-open" aria-hidden="true"/>
                <a href="/UserAccounts/Delete/@item.Id" class="glyphicon glyphicon-trash" aria-hidden="true"/>
            </td>
        </tr>
    }
</table>